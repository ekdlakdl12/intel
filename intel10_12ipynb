        "from sklearn.linear_model import Perceptron\n",
        "\n",
        "#훈련 집합 구축하기\n",
        "\n",
        "X=[[0,0],[0,1],[1,0],[1,1]]\n",
        "Y=[-1,1,1,1]\n",
        "\n",
        "#fit 함수로 Perceptron학습\n",
        "p = Perceptron()\n",
        "p.fit(X,Y)\n",
        "\n",
        "print(\"학습된 퍼셉트론의 매개변수\",p.coef_, p.intercept_)\n",
        "print(\"훈련집합에 대한 예측:\",p.predict(X))\n",
        "print(\"정확률 측정:\",p.score(X,Y)*100,\"%\")\n",
        "\n",
        "\n",
        "\n"
      ],
